<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.txg.project.mapper.LecturerMapper">
	<select id="lecturerLogin" parameterType="Lecturer" resultType="Lecturer">
		SELECT * FROM lecturer_info 
		WHERE lecturer_email = #{lecturerEmail} AND password = #{password} 
	</select>

</mapper>
